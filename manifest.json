{
  "name": "Re-emojify page content.",
  "description" : "Undoes image emoji on sites like Hangouts, GitHub, Slack, etc.",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [{
    "js": [
      "content.js"
    ],
    "matches": [
      "https://hangouts.google.com/*",
      "https://github.com/*",
      "https://*.slack.com/*"
    ],
    "all_frames": true
  }]
}
